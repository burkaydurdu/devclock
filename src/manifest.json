{
  "name": "devclock",
  "author": "Burkay Durdu",
  "description": "A new tab extension for Clock",
  "incognito": "split",
  "version": "0.0.1",
  "manifest_version": 2,
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": [
    {
      "src" : "/assets/icons/icon-32.png",
      "sizes" : "32x32",
      "type" : "image/png" 
    },
    {
      "src" : "/assets/icons/icon-64.png",
      "sizes" : "64x64",
      "type" : "image/png" 
    },
    {
      "src" : "/assets/icons/icon-128.png",
      "sizes" : "128x128",
      "type" : "image/png" 
    }
  ]
}